{
  "name": "@libmedia/avplayer-ui",
  "version": "1.2.2",
  "description": "libmedia avplayer ui",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaohappy/libmedia.git"
  },
  "scripts": {
    "prepublishOnly": "tsx ../../../scripts/check-publish-status.ts --package=avplayer-ui"
  },
