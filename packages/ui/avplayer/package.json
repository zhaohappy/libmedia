      "types": "./dist/types/src/AVPlayer.d.ts"
    },
    "./AVPlayer": {
      "types": "./dist/types/src/AVPlayer.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Gaoxing Zhao",
  "license": "LGPL-3.0-or-later",
  "dependencies": {
    "@libmedia/avplayer": "workspace:*",
    "yox": "^1.0.0-alpha.408"
  }
}