{
  "name": "libmedia",
  "version": "0.0.1",
  "description": "Web 平台音视频媒体处理库",
  "scripts": {
    "build-cheap-polyfill": "cross-env NODE_ENV=production tsx ./node_modules/webpack/bin/webpack.js --progress --env polyfill=1 release=1",
    "build-webassembly-runner": "cross-env NODE_ENV=production tsx ./node_modules/webpack/bin/webpack.js --progress --env webassembly_runner=1 release=1",
    "build-pcm-worklet-processor": "cross-env NODE_ENV=production tsx ./node_modules/webpack/bin/webpack.js --progress --env pcm_worklet_processor=1 release=1",
    "build-thread-entry": "cross-env NODE_ENV=production tsx ./node_modules/webpack/bin/webpack.js --progress --env thread_entry=1 release=1",
    "build-avplayer": "cross-env NODE_ENV=production tsx ./node_modules/webpack/bin/webpack.js --progress --env avplayer=1 release=1",
    "build-avplayer-legacy": "cross-env NODE_ENV=production tsx ./node_modules/webpack/bin/webpack.js --progress --env avplayer=1 release=1 legacy=1",
    "build-avplayer-dev": "cross-env NODE_ENV=development tsx --max-old-space-size=8192 ./node_modules/webpack/bin/webpack.js --progress --env avplayer=1 release=0",
    "build-avplayer-watch": "cross-env NODE_ENV=development tsx --max-old-space-size=8192 ./node_modules/webpack/bin/webpack.js --watch --progress --env avplayer=1 release=0",
    "build-avplayer-ui": "cross-env NODE_ENV=development tsx ./node_modules/webpack/bin/webpack.js --progress --env avplayer=1 ui=1 release=1",
    "build-avplayer-ui-dev": "cross-env NODE_ENV=development tsx ./node_modules/webpack/bin/webpack.js --progress --env avplayer=1 ui=1 release=0",
    "build-avtranscoder": "cross-env NODE_ENV=production tsx ./node_modules/webpack/bin/webpack.js --progress --env avtranscoder=1 release=1",
    "build-avtranscoder-dev": "cross-env NODE_ENV=development tsx ./node_modules/webpack/bin/webpack.js --progress --env avtranscoder=1 release=0",
    "build-avtranscoder-watch": "cross-env NODE_ENV=development tsx ./node_modules/webpack/bin/webpack.js --watch --progress --env avtranscoder=1 release=0",
    "doc": "typedoc --options ./typedoc.json",
    "server": "serve -p 8000 --cors --config ./test/serve.json",
    "eslint": "./node_modules/.bin/eslint -o=report.json -f=json './packages/**/*.ts' -c=./eslint/typescript.js --rulesdir=./eslint/rules --ignore-path=./.eslintignore",
    "eslint-fix": "./node_modules/.bin/eslint './packages/**/*.ts' './site/**/*.ts' './site/**/*.tsx' -c=./eslint/typescript.js --rulesdir=./eslint/rules --ignore-path=./.eslintignore --fix",
    "eslint-check": "./node_modules/.bin/eslint './packages/**/*.ts' -c=./eslint/typescript.js --rulesdir=./eslint/rules --ignore-path=./.eslintignore",
    "postinstall": "tsx scripts/postinstall.ts && simple-git-hooks"
  },
  "simple-git-hooks": {
    "commit-msg": "npx tsx scripts/verify-commit.ts",
    "pre-commit": "npx tsx scripts/pre-commit.ts"
  },
  "author": "Gaoxing Zhao",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@types/jest": "^23.3.14",
    "@types/loader-utils": "^2.0.6",
    "@types/node": "^22.10.1",
    "@types/path-browserify": "^1.0.0",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/yargs": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/typescript-estree": "^7.18.0",
    "@typescript-eslint/utils": "7.18.0",
    "autoprefixer": "^6.3.6",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "dts-bundle-webpack": "^1.0.2",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-visitor-keys": "^4.2.1",
    "espree": "^10.4.0",
    "esutils": "^2.0.3",
    "glslify-loader": "^2.0.0",
    "html2js": "^0.2.0",
    "loader-utils": "^2.0.0",
    "minimatch": "^10.1.1",
    "node-media-server": "^2.6.2",
    "picocolors": "^1.1.1",
    "postcss": "^5.0.19",
    "postcss-css-variables": "^0.17.0",
    "raw-loader": "^4.0.0",
    "rider": "^2.0.0",
    "schema-utils": "^3.1.0",
    "serve": "^14.2.4",
    "simple-git-hooks": "^2.11.1",
    "stylus": "^0.54.2",
    "terser": "^5.36.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^9.4.4",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.14",
    "typedoc-plugin-rename-defaults": "^0.7.3",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "worker-loader": "^3.0.8",
    "yargs": "^18.0.0"
  },
  "dependencies": {
    "ass-compiler": "^0.1.15",
    "assjs": "0.1.4",
    "commander": "^13.1.0",
    "yox": "^1.0.0-alpha.408"
  }
}
