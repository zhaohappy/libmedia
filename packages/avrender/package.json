      "default": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./registerProcessor": {
      "import": "./dist/esm/pcm/audioWorklet/base/registerProcessor.js",
      "default": "./dist/esm/pcm/audioWorklet/base/registerProcessor.js",
      "types": "./dist/esm/pcm/audioWorklet/base/registerProcessor.d.ts"
    },
    "./AudioSourceWorkletProcessorWorklet": "./dist/AudioSourceWorkletProcessorWorklet.js",
    "./AudioSourceWorkletProcessor2Worklet": "./dist/AudioSourceWorkletProcessor2Worklet.js"
  }
}